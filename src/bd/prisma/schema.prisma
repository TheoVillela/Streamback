generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id            Int      @id @default(autoincrement())
  login         String   @db.VarChar(255)
  email         String   @db.VarChar(255)
  password_hash String   @db.VarChar(255)
  isadmin       Boolean? @default(false)
}
